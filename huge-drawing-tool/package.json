{
  "name": "huge-drawing-tool",
  "version": "1.0.0",
  "description": "Simple Drawing Tool",
  "repository": {
    "type": "git",
    "url": "git@github.com:IT521/JobSearch.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "babel src -d build",
    "lint": "eslint src",
    "lintf": "eslint --fix src",
    "start": "npm run build && node build",
    "test": "npx jest",
    "test:vc": "npx jest --verbose --coverage",
    "test:update": "npx jest --verbose --coverage --updateSnapshot",
    "test:watch": "npx jest --verbose --watch",
    "coverage": "npx jest --collectCoverageFrom=src/**.js --collectCoverageFrom=!src/index.js --coverage src"
  },
  "author": "Terry S Francis",
  "license": "MIT",
  "keywords": [
    "huge coding challenge",
    "drawing tool"
  ],
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-arrow-functions": "^7.0.0",
    "@babel/plugin-transform-destructuring": "^7.1.3",
    "@babel/plugin-transform-flow-strip-types": "^7.1.6",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/plugin-transform-parameters": "^7.1.0",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/plugin-transform-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/runtime": "^7.1.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.0.1",
    "jest": "^23.6.0",
    "regenerator-runtime": "^0.12.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/register": "^7.0.0",
    "geometry-2d": "^0.2.1"
  }
}
